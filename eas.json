{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "EXPO_NO_CAPABILITY_SYNC": "1" }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "env": { "EXPO_NO_CAPABILITY_SYNC": "1" }
    },
    "preview": {
      "distribution": "internal",
      "env": { "EXPO_NO_CAPABILITY_SYNC": "1" }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "android": { "buildType": "app-bundle" },
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium",
        "env": {
          "RCT_NEW_ARCH_ENABLED": "1",
          "EXPO_NO_CAPABILITY_SYNC": "1"
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "mukesh@lynkd.app",
        "ascAppId": "6740008236"
      },
      "android": {
        "track": "production",
        "releaseStatus": "completed"
      }
    },
    "internal": {
      "android": {
        "track": "internal",
        "changesNotSentForReview": true
      }
    }
  }
}
