{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "EXPO_NO_CAPABILITY_SYNC": "1" }
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "env": { "EXPO_NO_CAPABILITY_SYNC": "1" }
    },
    "preview": {
      "distribution": "internal",
      "env": { "EXPO_NO_CAPABILITY_SYNC": "1" }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium",
        "env": {
          "RCT_NEW_ARCH_ENABLED": "1",
          "EXPO_NO_CAPABILITY_SYNC": "1"
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "mukesh@lynkd.app",
        "ascAppId": "6740008236"
      }
    }
  }
}
